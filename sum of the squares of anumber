#include<stdio.h>
#include<conio.h>
int sq(int c)
{
    return c*c;
}
        int sum(int (*sq) (int),int num1)
        {
            int total=0;
            for(int i=0;i<num1;i++){
                total=total+(*sq)(i);
                
            }
        }
        void main()
            {
                int n;
                printf("enter the num");
                scanf("%d",&n);
                sum(sq,n);
                
            }
        
        
